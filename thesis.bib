@article{seaton1954,
    title = "Excitation processes in the aurora and airglow 1. Absolute intensities, relative ultra-violet intensities and electron densities in high latitude aurorae",
    journal = "Journal of Atmospheric and Terrestrial Physics",
    volume = "4",
    number = "6",
    pages = "285 - 294",
    year = "1954",
    issn = "0021-9169",
    doi = "10.1016/0021-9169(54)90060-4",
    url = "http://www.sciencedirect.com/science/article/pii/0021916954900604",
    author = "M.J. Seaton",
    abstract = "The paper discusses certain auroral observations and seeks to obtain information in a form which is best suited to the study of physical processes. An attempt is made to express largely descriptive information on the auroral brightness in terms of a quantity π (5577), the number of oxygen green line quanta emitted, in the line of sight, per cm2 column per second. It is concluded that International Brightness Coefficients I, II, III and IV correspond to π (5577) being approximately equal to 2.109, 1.1010, 1.1011 and 5.1012 cm−2 sec−1. An extreme upper limit is estimated as 5.1015 cm−2 sec−1. Measured auroral intensities in ultra-violet vibration bands are compared with intensities obtained from theoretical and laboratory studies. By considering various v progressions (fixed upper state) it is shown that the auroral intensities are in error by an amount which increases systematically as the wavelength is decreased. An empirical correction curve is obtained. Radio and optical evidence on the electron density is discussed and it is concluded that electron densities between 107 and 108 cm−3 occur in bright high latitude aurorae."
}



@article{omholt1955,
    title = "Measurements of the mean lifetime of the metastable 1S-state of the oxygen atom in the upper atmosphere during auroral displays",
    journal = "Journal of Atmospheric and Terrestrial Physics",
    volume = "7",
    pages = "247--253",
    year = "1955",
    issn = "0021-9169",
    doi = "10.1016/0021-9169(55)90129-X",
    url = "http://www.sciencedirect.com/science/article/pii/002191695590129X",
    author = "A. Omholt and L. Harang",
    abstract = "The intensity variations of the green oxygen line 5577 Å (1D — 1S) and the first negative nitrogen band 4278 Å (N2+, B2∑u+ − X2∑g+) are measured in rapidly changing aurorae. The mean lifetime of the upper state of the nitrogen band is of the order 10−8 sec, the intensity variations of this band will probably therefore at the same time represent the instantaneous relative excitation rate of the metastable OI 1S-level. The mean lifetime of the metastable OI 1S-level is theoretically calculated to be of the order τ = 0·75 sec (Garstang, 1951). One should therefore expect a time-lag in the intensity curve of the 5577-line relative to the 4278-band. This effect is demonstrated on the records, and the mean lifetime of the OI 1S-level is found to vary between 0·45 and 0·75 sec. The upper value (0·75 sec) confirms the theoretical value found by Garstang (1951), the lower value found indicates that a probability of collisional deactivation of the metastable OI 1S-level of the order 1 sec−1 may appear in the auroral region of the upper atmosphere."
}

